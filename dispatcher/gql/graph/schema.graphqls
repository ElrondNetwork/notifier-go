# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Event {
  address: String
  identifier: String
  topics: [String]
  data: String
}

input SubscriptionEntry {
  address: String
  identifier: String
  topics: [String]
}

type Query {
  empty: String
}

type Subscription {
  subscribe(SubscriptionEntries: [SubscriptionEntry]): [Event]
}
